{
  "name": "registrace-app",
  "private": true,
  "scripts": {
    "dev": "wrangler pages dev ./public --d1=DB=registrace-db",
    "deploy": "wrangler pages deploy ./public",
    "db:migrate": "wrangler d1 execute registrace-db --file=./schema.sql",
    "db:migrate:local": "wrangler d1 execute registrace-db --local --file=./schema.sql",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241205.0",
    "typescript": "^5.3.3",
    "wrangler": "^3.99.0"
  }
}
